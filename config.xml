<?xml version='1.0' encoding='utf-8'?>
<widget id="com.adashkevich.phonegap.closein" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="phonegap-version" value="cli-8.0.0" />
    <name>Close In Client</name>
    <description>
        App for sharing and finding nearest events
    </description>
    <author email="andrew.daskhevich@gmail.com" href="http://cordova.io">
        Andrei Dashkevich, Natalia Oleynik, Kristina Kolbasyuk, Toma Suboch
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="res/xxxhdpi.png" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-device" spec="~2.0.1" />
    <plugin name="cordova-plugin-geolocation" spec="~4.0.0" />
</widget>
